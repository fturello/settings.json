{
	"editor.letterSpacing": 0.1,
	"editor.lineHeight": 22,
	"editor.fontSize": 14.3,
	"editor.fontWeight": "400",
	"editor.tabSize": 2,
	"editor.fontFamily": "'Cascadia Code', 'JetBrains Mono'",
	"editor.fontVariations": false,
	"editor.fontLigatures": false,
	"editor.formatOnSave": true,
	"editor.formatOnPaste": true,
	"editor.minimap.enabled": false,
	"editor.linkedEditing": true,
	"editor.acceptSuggestionOnCommitCharacter": false,
	"editor.renderWhitespace": "trailing",
	"editor.suggest.insertMode": "replace",
	"editor.wordWrap": "wordWrapColumn",
	"editor.cursorBlinking": "smooth",
	"editor.cursorSmoothCaretAnimation": "on",
	"editor.smoothScrolling": true,
	"editor.guides.bracketPairs": "active",
	"editor.bracketPairColorization.enabled": true,
	"explorer.autoReveal": false,
	"explorer.confirmDragAndDrop": false,
	"emmet.triggerExpansionOnTab": true,
	"emmet.includeLanguages": {
		"javascript": "javascriptreact",
		"ejs": "html"
	},
	"files.associations": {
		"*html": "html",
		"*.ejs": "html",
		"*javascript": "javascriptreact"
	},
	"files.defaultLanguage": "markdown",
	"files.autoSave": "afterDelay",
	"files.eol": "\n",
	"terminal.integrated.defaultProfile.windows": "Git Bash",
	"terminal.integrated.fontFamily": "JetBrainsMonoNL Nerd Font",
	"terminal.integrated.cursorStyle": "line",
	"window.zoomLevel": 0.9,
	"window.menuBarVisibility": "compact",
	"workbench.iconTheme": "material-icon-theme",
	"workbench.colorTheme": "Winter is Coming (Dark Blue - No Italics)",
	"workbench.list.smoothScrolling": true,
	"workbench.tree.indent": 15,
	"workbench.tree.renderIndentGuides": "always",
	"workbench.startupEditor": "none",
	"workbench.colorCustomizations": {
		"terminal.foreground": "#D0D0D0",
		"terminalCursor.background": "#D0D0D0",
		"terminalCursor.foreground": "#ffffff65",
		"terminal.ansiBlack": "#000000",
		"terminal.ansiBrightBlack": "#808080",
		"terminal.ansiBrightBlue": "#0066FF",
		"terminal.ansiBrightCyan": "#00FFFF",
		"terminal.ansiBrightGreen": "#00FF4C",
		"terminal.ansiBrightMagenta": "#CC00FF",
		"terminal.ansiBrightRed": "#FF0000",
		"terminal.ansiBrightWhite": "#FFFFFF",
		"terminal.ansiBrightYellow": "#F7FA3B",
		"terminal.ansiBlue": "#0066FF",
		"terminal.ansiCyan": "#00FFFF",
		"terminal.ansiGreen": "#23D18B",
		"terminal.ansiMagenta": "#A557FF",
		"terminal.ansiRed": "#FF4949",
		"terminal.ansiWhite": "#D0D0D0",
		"terminal.ansiYellow": "#FAC03B",
		"editorBracketHighlight.foreground1": "#f878b4",
		"editorBracketHighlight.foreground2": "#dfb976",
		"editorBracketHighlight.foreground3": "#c172d9",
		"editorBracketHighlight.foreground4": "#4fb1bc",
		"editorBracketHighlight.foreground5": "#97c26c",
		"editorBracketHighlight.foreground6": "#abb2c0",
		"editorBracketHighlight.unexpectedBracket.foreground": "#db6165",
		"[Winter is Coming (Dark Blue - No Italics)]": {
			"editor.selectionBackground": "#3D59A1",
			"editor.selectionHighlightBackground": "#3D59A1"
		},
		"[Tokyo Night]": {
			"editor.selectionBackground": "#3D59A1",
			"editor.selectionHighlightBackground": "#3D59A1"
		}
	},
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[css]": {
		"editor.defaultFormatter": "vscode.css-language-features"
	},
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[markdown]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"prettier.jsxSingleQuote": true,
	"prettier.useTabs": true,
	"gitlens.currentLine.enabled": false,
	"gitlens.codeLens.enabled": false,
	"gitlens.hovers.currentLine.over": "line",
	"liveServer.settings.donotShowInfoMsg": true,
	"multiCommand.commands": [
		{
			"command": "multiCommand.makeRoom",
			"sequence": [
				"workbench.action.toggleSidebarVisibility",
				"workbench.action.toggleActivityBarVisibility"
			]
		}
	]
}
